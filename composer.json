{
  "name": "cretueusebiu/laravel-vue-spa",
  "description": "A Laravel-Vue SPA starter project template.",
  "keywords": [
    "spa",
    "laravel",
    "vue"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "algolia/scout-extended": "^1.8",
    "barryvdh/laravel-cors": "^0.11.2",
    "barryvdh/laravel-dompdf": "^0.8.5",
    "cossou/jasperphp": "^2.8",
    "fideloper/proxy": "^4.0",
    "geekcom/phpjasper": "^3.3",
    "guzzlehttp/guzzle": "^6.3",
    "intervention/image": "^2.5",
    "jimmyjs/laravel-report-generator": "^2.0",
    "laravel/framework": "5.7.*",
    "laravel/scout": "^7.2",
    "laravel/socialite": "^3.0",
    "laravel/tinker": "~1.0",
    "laravellegends/pt-br-validator": "5.1.*",
    "lucascudo/laravel-pt-br-localization": "^1.0",
    "nexmo/laravel": "^2.3",
    "pusher/pusher-php-server": "~3.0",
    "spatie/laravel-medialibrary": "^7.0.0",
    "spatie/laravel-permission": "^2.25",
    "tymon/jwt-auth": "^1.0.0-rc.2"
  },
  "require-dev": {
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "laravel/dusk": "^3.0",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/dusk"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate",
      "@php artisan jwt:secret --force"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
